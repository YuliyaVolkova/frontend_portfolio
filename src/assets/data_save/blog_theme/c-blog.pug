.c-blog
	aside.c-blog__aside
		nav.c-blog__nav
			ul.c-blog__nav-list
				each val, index in posts
					case index
						when 0	
							li.c-blog__nav-item.is-active
								a.c-blog__nav-link(href=`#${index+1}article`)= val.title
						default
							li.c-blog__nav-item
								a.c-blog__nav-link(href=`#${index+1}article`)= val.title
	main.c-blog__main
		each val, index in posts
			article.c-blog__article(data-section=`#${index+1}article`)
				h3.h-title-article
					span= val.title
				time.article__date(datetime="")= val.date
				.article__text
					span!= val.text