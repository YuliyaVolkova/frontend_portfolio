
- var social = ['vk', 'github', 'in']
- var arrowId = 'arrow_down'

	+social('l-hero', social, '../assets/images/sprites/sprite.svg')
	+icon(bgTitleSvgId, `svg_hero`, '../assets/images/sprites/sprite.svg')
	.l-hero__develop
		.c-developer
			.c-developer__img-wrap(style="background-image: url('../assets/images/content/photo.jpg'); background-repeat:no-repeat;")
				img.c-developer__img.visually-hidden(alt="", src=require('../assets/images/content/photo.jpg'))
			.c-developer__content
				h1.c-developer__header Yuliya Volkova
				h2.c-developer__desc Личный сайт WEB Разработчика
	a.c-next#next(href='#')
		+icon(arrowId, 'svg_next', '../assets/images/sprites/sprite.svg')
	.l-hero__bottom
		+icon('bg', 'svg_hero-bottom', '../assets/images/sprites/sprite.svg')
		block content